      <artifactId>platform-component-edition-enterprise</artifactId>
    </dependency>
    <dependency>
      <groupId>org.exoplatform.platform</groupId>
      <artifactId>platform-component-gadgets</artifactId>
    </dependency>
    <dependency>
      <groupId>org.exoplatform.platform</groupId>
      <artifactId>platform-component-organization</artifactId>
    </dependency>
    <dependency>
      <groupId>org.exoplatform.platform</groupId>
      <artifactId>platform-component-webui</artifactId>
    </dependency>
    <dependency>
      <groupId>org.exoplatform.platform</groupId>
      <artifactId>platform-extension-config</artifactId>
    </dependency>
    <dependency>
      <groupId>org.exoplatform.platform</groupId>
      <artifactId>platform-extension-portlets-platformNavigation</artifactId>
      <type>war</type>
    </dependency>
    <dependency>
      <groupId>org.exoplatform.platform</groupId>
      <artifactId>platform-extension-resources</artifactId>
      <type>war</type>
    </dependency>
    <dependency>
      <groupId>org.exoplatform.platform</groupId>
      <artifactId>platform-extension-webapp</artifactId>
      <type>war</type>
    </dependency>
    <dependency>
      <groupId>org.exoplatform.platform</groupId>
      <artifactId>platform-sample-gadgets-sample-config</artifactId>
    </dependency>
    <dependency>
      <groupId>org.exoplatform.platform</groupId>
      <artifactId>platform-sample-gadgets-sample-exo-gadget-resources</artifactId>
      <type>war</type>
    </dependency>
    <!-- Gadgets on the intranet homepage -->
    <dependency>
      <groupId>org.exoplatform.platform</groupId>
      <artifactId>platform-sample-gadgets-sample-gadgets</artifactId>
      <type>war</type>
    </dependency>
    <dependency>
      <groupId>org.exoplatform.platform</groupId>
      <artifactId>platform-sample-gadgets-sample-service</artifactId>
    </dependency>
  </dependencies>
  <build>
    <finalName>platform-extension</finalName>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-ear-plugin</artifactId>
        <configuration>
          <displayName>platform-extension</displayName>
          <defaultLibBundleDir>lib</defaultLibBundleDir>
          <modules>
            <webModule>
              <groupId>org.exoplatform.platform</groupId>
              <artifactId>platform-extension-webapp</artifactId>
              <contextRoot>platform-extension</contextRoot>
              <bundleFileName>platform-extension.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>org.exoplatform.platform</groupId>
              <artifactId>platform-extension-resources</artifactId>
              <contextRoot>eXoPlatformResources</contextRoot>
              <bundleFileName>eXoPlatformResources.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>org.exoplatform.platform</groupId>
              <artifactId>platform-extension-portlets-platformNavigation</artifactId>
              <contextRoot>platformNavigation</contextRoot>
              <bundleFileName>platformNavigation.war</bundleFileName>
            </webModule>
            <!-- IDE -->
            <webModule>
              <groupId>org.exoplatform.ide</groupId>
              <artifactId>exo-ide-client-gadget</artifactId>
              <contextRoot>IDE</contextRoot>
              <bundleFileName>IDE.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>org.exoplatform.gwt</groupId>
              <artifactId>exo-gwtframework-smartgwt</artifactId>
              <contextRoot>SmartGWT</contextRoot>
              <bundleFileName>SmartGWT.war</bundleFileName>
            </webModule>
            <jarModule>
              <groupId>org.exoplatform.ide</groupId>
              <artifactId>exo-ide-extension-gadget-server</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <jarModule>
              <groupId>org.exoplatform.ide</groupId>
              <artifactId>exo-ide-extension-netvibes-server</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <jarModule>
              <groupId>org.exoplatform.ide</groupId>
              <artifactId>exo-ide-extension-groovy-server</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <jarModule>
              <groupId>org.exoplatform.ide</groupId>
              <artifactId>exo-ide-server</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <jarModule>
              <groupId>org.exoplatform.core</groupId>
              <artifactId>exo.core.component.script.groovy</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <jarModule>
              <groupId>org.exoplatform.ide</groupId>
              <artifactId>exo-ide-extension-chromattic-server</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <jarModule>
              <groupId>org.chromattic</groupId>
              <artifactId>chromattic.groovy</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <jarModule>
              <groupId>org.chromattic</groupId>
              <artifactId>chromattic.dataobject</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <!-- Commons -->
            <jarModule>
              <groupId>org.exoplatform.commons</groupId>
              <artifactId>commons-webui-ext</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <jarModule>
              <groupId>org.exoplatform.commons</groupId>
              <artifactId>commons-webui-component</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <jarModule>
              <groupId>org.exoplatform.commons</groupId>
              <artifactId>commons-comet-service</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <webModule>
              <groupId>org.exoplatform.commons</groupId>
              <artifactId>commons-comet-webapp</artifactId>
              <contextRoot>cometd</contextRoot>
              <bundleFileName>cometd.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>org.exoplatform.commons</groupId>
              <artifactId>commons-fck</artifactId>
              <contextRoot>fck</contextRoot>
              <bundleFileName>fck.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>org.exoplatform.commons</groupId>
              <artifactId>commons-webui-resources</artifactId>
              <contextRoot>CommonsResources</contextRoot>
              <bundleFileName>CommonsResources.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>org.exoplatform.commons</groupId>
              <artifactId>commons-extension-webapp</artifactId>
              <contextRoot>commons-extension</contextRoot>
              <bundleFileName>commons-extension.war</bundleFileName>
            </webModule>
            <!-- Extension config -->
            <jarModule>
              <groupId>org.exoplatform.platform</groupId>
              <artifactId>platform-extension-config</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <jarModule>
              <groupId>org.exoplatform.platform</groupId>
              <artifactId>platform-component-edition-enterprise</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <!-- Gagdets on the intranet homepage-->
            <jarModule>
              <groupId>org.exoplatform.platform</groupId>
              <artifactId>platform-sample-gadgets-sample-config</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <jarModule>
              <groupId>org.exoplatform.platform</groupId>
              <artifactId>platform-sample-gadgets-sample-service</artifactId>
              <includeInApplicationXml>true</includeInApplicationXml>
            </jarModule>
            <webModule>
              <groupId>org.exoplatform.platform</groupId>
              <artifactId>platform-sample-gadgets-sample-gadgets</artifactId>
              <contextRoot>intranet-gadgets</contextRoot>
              <bundleFileName>intranet-gadgets.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>org.exoplatform.platform</groupId>
              <artifactId>platform-sample-gadgets-sample-exo-gadget-resources</artifactId>
              <contextRoot>exo-gadget-resources</contextRoot>
              <bundleFileName>exo-gadget-resources.war</bundleFileName>
            </webModule>
            <!-- WAI portal template-->
            <webModule>
              <groupId>org.exoplatform.platform</groupId>
              <artifactId>platform-sample-wai-template</artifactId>
              <contextRoot>platform-sample-wai-template</contextRoot>
              <bundleFileName>platform-wai-template.war</bundleFileName>
            </webModule>
          </modules>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
