<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIAdminToolbarPortlet>
      <action>
         <AddPage>Add Page</AddPage>
         <EditNavigation>Navigation</EditNavigation>
         <Content>Content</Content>
         <Editor>Edit</Editor>
         <Page>Page</Page>
         <Site>Site</Site>
         <Layout>Edit Layout</Layout>
         <ManagePages>Manage Pages</ManagePages>
         <GroupSite>Group Site</GroupSite>
         <Properties>SEO</Properties>
         <AddSite>Add Site</AddSite>
      </action>
		<ManageSites>Manage sites</ManageSites>
		<ManageGroupSites>Manage group sites</ManageGroupSites>
   </UIAdminToolbarPortlet>

  <UISEOForm>
    <label>
      <title>Title: </title>  
      <description>Description: </description>
      <keywords>Keywords: </keywords>
      <language>Language: </language>
      <robots>Robots: </robots>
      <sitemap>Sitemap: </sitemap>
      <sitemapvisible>Visible in sitemap</sitemapvisible>
	  <isInherited>Inherited from parent page</isInherited>
      <priority>Priority: </priority>
      <frequency>Frequency: </frequency>
      <select-language>Select a language</select-language>
      <option>
        <Always>Always</Always>
        <Hourly>Hourly</Hourly>
        <Daily>Daily</Daily>
        <Weekly>Weekly</Weekly>
        <Monthly>Monthly</Monthly>
        <Yearly>Yearly</Yearly>
        <Never>Never</Never>
      </option> 
    </label>
 
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
    <msg>
      <repository-exception>An unexpected error occurred! Cannot save page meta data.</repository-exception>
      <confirm-delete>Are you sure you want to delete these SEO data?</confirm-delete>	
    </msg>
    <title>
    	<seo-delete>Delete</seo-delete>
    </title>
	<help> 
	  <priority>Valid values range from 0.0 to 1.0.</priority>
	  <description>Help searchers to let them know what the following page has a link with what they are looking for.</description>
	  <keywords>Most important terms in the page separated by a comma.</keywords>		  
    </help>  
  </UISEOForm>
  <UISEOPopupWindow>
  	<title>
      <UISEOForm>SEO Management</UISEOForm>
  	</title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  	<title>
      <seo>SEO Management</seo>
  	</title>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>The field Priority is invalid float number. Valid values range from 0.0 to 1.0.</Invalid-number>
    </msg>
  </FloatNumberValidator>
  
  <Locale>
    <label>      
      <it>Italian</it>      
      <fr>French</fr>
      <en>English</en>      
    </label>
  </Locale>
  
</bundle>
