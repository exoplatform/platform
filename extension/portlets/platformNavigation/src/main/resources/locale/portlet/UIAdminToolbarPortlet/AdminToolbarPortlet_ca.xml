<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIAdminToolbarPortlet>
      <action>
         <AddPage>Afegeix una pàgina</AddPage>
         <EditNavigation>Navegació</EditNavigation>
         <Content>Contingut</Content>
         <Editor>Edita</Editor>
         <Page>Pàgina</Page>
         <Site>Lloc</Site>
         <Layout>Edita la Disposició</Layout>
         <siteLayout>Disseny</siteLayout>
         <ManagePages>Gestiona les Pàgines</ManagePages>
         <GroupSite>Grup del Lloc</GroupSite>
         <Properties>#{UISEOToolbarForm.label.seo}</Properties>
         <AddSite>Afegeix Lloc</AddSite>
      </action>
<ManageSites>Gestiona Llocs</ManageSites>
<ManageGroupSites>Gestiona el Grup dels Llocs</ManageGroupSites>
   </UIAdminToolbarPortlet>

  <UISEOForm>
    <label>
      <title>Títol:</title>  
      <description>Descripció:</description>
      <keywords>Paraules clau:</keywords>
      <language>Idioma:</language>
      <robots>Robots:</robots>
      <sitemap>Mapa del lloc:</sitemap>
      <sitemapvisible>Visible al Mapa del Lloc</sitemapvisible>
  <isInherited>Heretat de la pàgina principal</isInherited>
      <priority>Prioritat:</priority>
      <frequency>Freqüència:</frequency>
      <select-language>Selecciona un Idioma</select-language>
      <option>
        <Always>Sempre</Always>
        <Hourly>Per hora</Hourly>
        <Daily>Diàriament</Daily>
        <Weekly>Setmanalment</Weekly>
        <Monthly>Mensualment</Monthly>
        <Yearly>Anualment</Yearly>
        <Never>Mai</Never>
      </option> 
    </label>
 
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <repository-exception>S'ha produït un error inesperat. No es pot desar les metadades de la pàgina.</repository-exception>
      <confirm-delete>Està segur que vol suprimir aquestes dades SEO?</confirm-delete>
    </msg>
    <title>
    <seo-delete>Suprimeix</seo-delete>
    </title>
<help> 
  <priority>Gamma de valors vàlids de 0.0 a 1.0.</priority>
  <description>Ajuda als usuaris de motors de recerca mostrant-los que aquesta pàgina té un enllaç al que estan buscant.</description>
  <keywords>Els termes més importants de la pàgina han d'estar separats per una coma.</keywords>  
    </help>  
  </UISEOForm>
  <UISEOPopupWindow>
  <title>
      <UISEOForm>#{UISEOToolbarForm.title.seo}</UISEOForm>
  </title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  <title>
      <seo>Gestió de SEO</seo>
  </title>
  <label>
    <seo>SEO</seo>
  </label>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>El camp Prioritat té un valor invàlid. Introdueix un número entre 0.0 i 1.0.</Invalid-number>
    </msg>
  </FloatNumberValidator>
  
  <Locale>
    <label>      
      <it>Italià</it>      
      <fr>Francès</fr>
      <en>Anglès</en>      
    </label>
  </Locale>
  
</bundle>
