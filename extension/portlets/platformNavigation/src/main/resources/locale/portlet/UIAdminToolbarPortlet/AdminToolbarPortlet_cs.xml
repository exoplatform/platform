      <frequency>Frekvence:</frequency>
      <select-language>Vybrat jazyk</select-language>
      <option>
        <Always>Vždy</Always>
        <Hourly>Každou hodinu</Hourly>
        <Daily>Denně</Daily>
        <Weekly>Týdně</Weekly>
        <Monthly>Měsíčně</Monthly>
        <Yearly>Ročně</Yearly>
        <Never>Nikdy</Never>
      </option> 
    </label>
 
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <repository-exception>Došlo k neočekávané chybě. Metadata stránky nelze uložit.</repository-exception>
      <confirm-delete>Jste si jisti, že chcete odstranit tyto údaje SEO?</confirm-delete>
    </msg>
    <title>
    <seo-delete>Odstranit</seo-delete>
    </title>
<help> 
  <priority>Platné hodnoty jsou v rozsahu od 0.0 do 1.0.</priority>
  <description>Pomozte hledajícím: ať vědí, že na následující stránce se nachází odkaz na stránku, kterou hledají.</description>
  <keywords>Nejdůležitější termíny na stránce musí být odděleny čárkou.</keywords>  
    </help>  
  </UISEOForm>
  <UISEOPopupWindow>
  <title>
      <UISEOForm>#{UISEOToolbarForm.title.seo}</UISEOForm>
  </title>
