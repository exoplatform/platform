      <priority>Prioritet:</priority>
      <frequency>Frekvens:</frequency>
      <select-language>Velg språk</select-language>
      <option>
        <Always>Alltid</Always>
        <Hourly>Hver time</Hourly>
        <Daily>Daglig</Daily>
        <Weekly>Ukentlig</Weekly>
        <Monthly>Månedlig</Monthly>
        <Yearly>Årlig</Yearly>
        <Never>Aldri</Never>
      </option> 
    </label>
 
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <repository-exception>Det oppstod en uventet feil. Kan ikke lagre sidens metadata.</repository-exception>
      <confirm-delete>Er du sikker på at du vil slette disse SEO data?</confirm-delete>
    </msg>
    <title>
    <seo-delete>Slette</seo-delete>
    </title>
<help> 
  <priority>Gyldige verdier er  fra 0,0 til 1,0.</priority>
  <description>Hjelp de som søker: la dem få vite at denne side har en link med det de leter etter.</description>
  <keywords>De viktigste ordene og uttrykkene på siden må være atskilt med semikolon.</keywords>  
    </help>  
  </UISEOForm>
  <UISEOPopupWindow>
  <title>
      <UISEOForm>#{UISEOToolbarForm.title.seo}</UISEOForm>
  </title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  <title>
      <seo>SEO Management</seo>
  </title>
  <label>
    <seo>SEO</seo>
  </label>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>Feltet prioritet har en ugyldig verdi. Angi et nummer mellom 0.0 og 1.0.</Invalid-number>
    </msg>
  </FloatNumberValidator>
  
  <Locale>
    <label>      
      <it>Italiensk</it>      
      <fr>Fransk</fr>
      <en>Engelsk</en>      
    </label>
  </Locale>
  
