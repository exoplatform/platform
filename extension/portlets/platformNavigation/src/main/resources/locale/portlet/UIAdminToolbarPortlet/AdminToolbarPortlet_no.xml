         <Page>Side</Page>
         <Site>Nettsted</Site>
         <Layout>Rediger layout</Layout>
         <siteLayout>Layout</siteLayout>
         <ManagePages>Administrere sider</ManagePages>
         <GroupSite>Gruppe nettsider</GroupSite>
