<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIAdminToolbarPortlet>
      <action>
         <AddPage>Dodaj stronę</AddPage>
         <EditNavigation>Edit Navigation</EditNavigation>
         <Content>Edit Content</Content>
         <Editor>Edytuj</Editor>
         <Page>Strona</Page>
         <Site>Witryna</Site>
         <Layout>Edytuj Układ</Layout>
         <siteLayout>Edit Layout</siteLayout>
         <ManagePages>Zarządzaj stronami</ManagePages>
         <GroupSite>Strona grupy</GroupSite>
         <Properties>#{UISEOToolbarForm.label.seo}</Properties>
         <AddSite>Dodaj witrynę</AddSite>
      </action>
<ManageSites>Zarządzaj stronami</ManageSites>
<ManageGroupSites>Zarządzaj stronami grupy</ManageGroupSites>
   </UIAdminToolbarPortlet>

  <UISEOForm>
    <label>
      <title>Tytuł:</title>  
      <description>Description:</description>
      <keywords>Słowa kluczowe:</keywords>
      <language>Język:</language>
      <robots>Roboty:</robots>
      <sitemap>Mapa strony:</sitemap>
      <sitemapvisible>Widoczne w mapie strony</sitemapvisible>
  <isInherited>Dziedzicz ze strony nadrzędnej</isInherited>
      <priority>Priorytet:</priority>
      <frequency>Częstotliwość:</frequency>
      <select-language>Wybierz język</select-language>
      <option>
        <Always>Zawsze</Always>
        <Hourly>Co godzinę</Hourly>
        <Daily>Codziennie</Daily>
        <Weekly>Co tydzień</Weekly>
        <Monthly>Co miesiąc</Monthly>
        <Yearly>Rocznie</Yearly>
        <Never>Nigdy</Never>
      </option> 
    </label>
 
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <repository-exception>Wystąpił nieoczekiwany błąd. Nie można zapisać metadanych strony.</repository-exception>
      <confirm-delete>Czy na pewno chcesz usunąć te dane SEO?</confirm-delete>
    </msg>
    <title>
    <seo-delete>Usuń</seo-delete>
    </title>
<help> 
  <priority>Prawidłowe wartości to od 0.0 do 1.0.</priority>
  <description>Pomóż wyszukującym: niech wiedzą, że poniższa strona ma związek z tym, czego szukają.</description>
  <keywords>Najważniejsze wyrażenia na stronie muszą być oddzielone przecinkami.</keywords>  
    </help>  
  </UISEOForm>
  <UISEOPopupWindow>
  <title>
      <UISEOForm>#{UISEOToolbarForm.title.seo}</UISEOForm>
  </title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  <title>
      <seo>Zarządzanie SEO</seo>
  </title>
  <label>
    <seo>SEO</seo>
  </label>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>Pole priorytet ma nieprawidłową wartość. Wprowadź liczbę pomiędzy 0,0 a 1,0.</Invalid-number>
    </msg>
  </FloatNumberValidator>
  
  <Locale>
    <label>      
      <it>włoski</it>      
      <fr>francuski</fr>
      <en>angielski</en>      
    </label>
  </Locale>
  
</bundle>
