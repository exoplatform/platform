<gatein-resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_resources_1_3 http://www.gatein.org/xml/ns/gatein_resources_1_3"
                  xmlns="http://www.gatein.org/xml/ns/gatein_resources_1_3">

    <portlet-skin>
        <application-name>platformNavigation</application-name>
        <portlet-name>UICompanyNavigationPortlet</portlet-name>
        <skin-name>Default</skin-name>
        <css-path>/skin/platformNavigation/UICompanyNavigationPortlet/DefaultStylesheet.css</css-path>
    </portlet-skin>
    <portlet-skin>
        <application-name>platformNavigation</application-name>
        <portlet-name>UIBreadCrumbsNavigationPortlet</portlet-name>
        <skin-name>Default</skin-name>
        <css-path>/skin/platformNavigation/UIBreadCrumbsNavigationPortlet/DefaultStylesheet.css</css-path>
    </portlet-skin>
    <portlet-skin>
        <application-name>platformNavigation</application-name>
        <portlet-name>UIGroupsNavigationPortlet</portlet-name>
        <skin-name>Default</skin-name>
        <css-path>/skin/platformNavigation/UIGroupsNavigationPortlet/DefaultStylesheet.css</css-path>
    </portlet-skin>
    <portlet-skin>
        <application-name>platformNavigation</application-name>
        <portlet-name>UIHelpPlatformToolbarPortlet</portlet-name>
        <skin-name>Default</skin-name>
        <css-path>/skin/platformNavigation/UIHelpPlatformToolbarPortlet/DefaultStylesheet.css</css-path>
    </portlet-skin>
    <portlet-skin>
        <application-name>platformNavigation</application-name>
        <portlet-name>UISearchPlatformToolbarPortlet</portlet-name>
        <skin-name>Default</skin-name>
        <css-path>/skin/platformNavigation/UISearchPlatformToolbarPortlet/DefaultStylesheet.css</css-path>
    </portlet-skin>
    <portlet-skin>
        <application-name>platformNavigation</application-name>
        <portlet-name>UISpaceNavigationPortlet</portlet-name>
        <skin-name>Default</skin-name>
        <css-path>/skin/platformNavigation/UISpaceNavigationPortlet/DefaultStylesheet.css</css-path>
    </portlet-skin>
    <portlet-skin>
        <application-name>platformNavigation</application-name>
        <portlet-name>UIUserNavigationPortlet</portlet-name>
        <skin-name>Default</skin-name>
        <css-path>/skin/platformNavigation/UIUserNavigationPortlet/DefaultStylesheet.css</css-path>
    </portlet-skin>
    <portlet>
        <name>PlatformAdminToolbarPortlet</name>
        <module>
            <depends>
                <module>bts_dropdown</module>
            </depends>
            <depends>
              <module>bts_hover_dropdown</module>
            </depends>
        </module>
    </portlet>
    <portlet>
        <name>UISetupPlatformToolBarPortlet</name>
        <module>
            <depends>
                <module>bts_dropdown</module>
            </depends>
            <depends>
                <module>bts_hover_dropdown</module>
            </depends>
        </module>
    </portlet>
    <portlet>
        <name>UIUserInfoPortlet</name>
        <module>
            <depends>
                <module>base</module>
            </depends>
           <depends>
              <module>bts_dropdown</module>
           </depends>
           <depends>
              <module>bts_hover_dropdown</module>
           </depends>
            <!--
            <depends>
                <module>socialcomposer</module>
            </depends>
            -->
            <depends>
                <module>social-core</module>
            </depends>
        </module>
    </portlet>

    <portlet>
        <name>UICreatePlatformToolBarPortlet</name>
        <module>
            <depends>
                <module>navigation-toolbar</module>
            </depends>
            <depends>
                <module>base</module>
            </depends>
            <depends>
                <module>bts_dropdown</module>
            </depends>
            <depends>
                <module>bts_hover_dropdown</module>
            </depends>
        </module>
    </portlet>
    <!--Begin-->
    <portlet>
        <name>UIHelpPlatformToolBarPortlet</name>
        <module>
            <depends>
                <module>base</module>
            </depends>

            <depends>
                <module>social-core</module>
            </depends>
        </module>
    </portlet>
    <!--END-->
    <portlet>
        <name>UIUserNavigationPortlet</name>
        <module>
            <depends>
                <module>webui-ext</module>
            </depends>
        </module>
    </portlet>
    <portlet>
        <name>UICompanyNavigationPortlet</name>
        <module>
            <depends>
                <module>navigation</module>
            </depends>
        </module>
    </portlet>
    <portlet>
        <name>UISearchPlatformToolbarPortlet</name>
        <module>
            <script>
                <path>/javascript/eXo/platform/search/Search.js</path>
            </script>
            <depends>
                <module>jquery</module>
                <as>$</as>
            </depends>
        </module>
    </portlet>
    <portlet>
        <name>UIGroupsNavigationPortlet</name>
        <scripts>
            <script>
                <path>/javascript/eXo/platform/navigation/UIGroupsNavigationPortlet/UIGroupsNavigation.js</path>
            </script>
            <depends>
                <scripts>bootstrap</scripts>
            </depends>
        </scripts>
    </portlet>
    <portlet>
        <name>UIBreadCrumbsNavigationPortlet</name>
        <module>
            <depends>
                <module>navigation</module>
            </depends>
        </module>
    </portlet>
    <module>
        <name>navigation-spaces-search</name>
       <load-group>plf_navigation</load-group>
        <script>
            <path>/javascript/eXo/platform/navigation/UISpaceNavigationPortlet/UISpaceNavigation.js</path>
        </script>
        <depends>
            <module>jquery</module>
            <as>$</as>
        </depends>
    </module>

   <module>
      <name>navigation-toolbar</name>
      <load-group>plf_navigation</load-group>
      <script>
         <path>/javascript/eXo/platform/navigation/ToolBarNavigation/UIToolBarNavigation.js</path>
      </script>
      <depends>
      <module>base</module>
   </depends>
      <depends>
         <module>uiPopup</module>
      </depends>
      <depends>
         <module>portalRequest</module>
      </depends>
      <depends>
         <module>jquery</module>
      </depends>
   </module>

   <module>
      <name>bts_hover_dropdown</name>
      <load-group>plf_navigation</load-group>
      <script>
         <path>/javascript/eXo/platform/navigation/ToolBarNavigation/twitter-bootstrap-hover-dropdown.js</path>
      </script>
      <depends>
         <module>jquery</module>
         <as>jQuery</as>
      </depends>
   </module>
</gatein-resources>
