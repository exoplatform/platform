<portlet-app version="2.0" xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">

  <portlet>
    <portlet-name>Bookmark</portlet-name>
	<portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
	<init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/bookmark/index.html</value>
    </init-param>
	<supports>
	  <mime-type>text/html</mime-type>
	</supports>
	  <supported-locale>en</supported-locale>
	  <supported-locale>fr</supported-locale>
	  <resource-bundle>locale.portlet.bookmark.BookmarkPortlet</resource-bundle>
	  <portlet-info>
	    <title>Bookmark</title>
	  </portlet-info>
  </portlet>
  <portlet>
	<portlet-name>LoginHistory</portlet-name>
	<portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
    <init-param>
	  <name>portlet-view-dispatched-file-path</name>
	  <value>/login-history/index.html</value>
    </init-param>
	<supports>
      <mime-type>text/html</mime-type>
	</supports>
	<supported-locale>en</supported-locale>
	<supported-locale>fr</supported-locale>
	<resource-bundle>locale.portlet.login.history.LoginHistory</resource-bundle>
	<portlet-info>
      <title>LoginHistory</title>
	</portlet-info>
  </portlet>
</portlet-app>
