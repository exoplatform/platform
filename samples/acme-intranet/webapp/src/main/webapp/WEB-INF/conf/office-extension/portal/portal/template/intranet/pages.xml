  <page>
		<name>homepage</name>
		<title>Home Page</title>
		<access-permissions>*:/platform/users</access-permissions>
		<edit-permission>*:/platform/administrators</edit-permission>
		<container id="Officebody" template="system:/groovy/portal/webui/container/UITableColumnContainer.gtmpl">
			<access-permissions>*:/platform/users</access-permissions>
			<container id="OfficeLeft" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
				<access-permissions>*:/platform/users</access-permissions>
                <!-- My Profile gadget -->
		        <portlet-application>
					<portlet>
						<application-ref>dashboard</application-ref>
						<portlet-ref>GadgetPortlet</portlet-ref>
						<preferences>
							<preference>
								<name>url</name>
								<value>local://Profile</value>
								<read-only>false</read-only>
							</preference>
						</preferences>
					</portlet>
					<title>My Profile</title>
					<access-permissions>*:/platform/users</access-permissions>
					<show-info-bar>false</show-info-bar>
					<show-application-state>true</show-application-state>
				</portlet-application>
				
				<!-- Invitations gadget -->
		        <portlet-application>
					<portlet>
						<application-ref>dashboard</application-ref>
						<portlet-ref>GadgetPortlet</portlet-ref>
						<preferences>
							<preference>
								<name>url</name>
								<value>local://Invitations</value>
								<read-only>false</read-only>
							</preference>
						</preferences>
					</portlet>
					<title>Invitations</title>
					<access-permissions>*:/platform/users</access-permissions>
					<show-info-bar>false</show-info-bar>
					<show-application-state>true</show-application-state>
				</portlet-application>
				
                <!-- Agenda gadget -->
		        <portlet-application>
					<portlet>
						<application-ref>dashboard</application-ref>
						<portlet-ref>GadgetPortlet</portlet-ref>
						<preferences>
							<preference>
								<name>url</name>
								<value>local://Agenda</value>
								<read-only>false</read-only>
							</preference>
						</preferences>
					</portlet>
					<title>Agenda</title>
					<access-permissions>*:/platform/users</access-permissions>
					<show-info-bar>false</show-info-bar>
					<show-application-state>true</show-application-state>
				</portlet-application>

                <!-- Feature poll gadget -->
		        <portlet-application>
					<portlet>
						<application-ref>dashboard</application-ref>
						<portlet-ref>GadgetPortlet</portlet-ref>
