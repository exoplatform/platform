            <application-ref>presentation</application-ref>
            <portlet-ref>ContentListViewerPortlet</portlet-ref>
            <preferences>
              <preference>
                <name>folderPath</name>
                <value>repository:collaboration:/sites content/live/@owner@/web contents/site artifacts/announcements</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>orderType</name>
                <value>ASC</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>formViewTemplatePath</name>
                <value>/exo:ecm/views/templates/content-list-viewer/list/announcement.gtmpl</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>basePath</name>
                <value>detail</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>itemsPerPage</name>
                <value>100</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>showTitle</name>
                <value>false</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>showDateCreated</name>
                <value>false</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>showHeader</name>
                <value>false</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>showLink</name>
                <value>false</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>showRssLink</name>
                <value>false</value>
                <read-only>false</read-only>
              </preference>     
            </preferences>
         </portlet>
          <title>Announcement</title>
          <access-permissions>*:/platform/users</access-permissions>
          <show-info-bar>false</show-info-bar>
          <show-application-state>false</show-application-state>
          <show-application-mode>false</show-application-mode>
        </portlet-application>
        
	    <!-- user activities portlet -->
		<portlet-application>
				<portlet>
						<application-ref>social-portlet</application-ref>
