            <application-ref>presentation</application-ref>
            <portlet-ref>SingleContentViewer</portlet-ref>
            <preferences>
              <preference>
                <name>workspace</name>
                <value>collaboration</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>nodeIdentifier</name>
                <value>/sites content/live/@owner@/web contents/site artifacts/Footer</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>ShowQuickEdit</name>
                <value>true</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>ShowPrintAction</name>
                <value>false</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>ShowTOC</name>
                <value>false</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>ShowTags</name>
                <value>false</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>ShowCategories</name>
                <value>false</value>
                <read-only>false</read-only>
              </preference>
              <preference>
                <name>ShowTitle</name>
                <value>false</value>
                <read-only>false</read-only>
              </preference>
            </preferences>
          </portlet>
          <title>Footer</title>
