<?xml version="1.0" encoding="UTF-8"?>
<bundle>
	<UIActionBar>
		<tooltip>
			<ContentNavigation>Navigation</ContentNavigation>
		</tooltip>
	</UIActionBar>
	<UIViewFormTabPane>
		<label>
			<contentNavigation>Navigation par contenu</contentNavigation>
		</label>
	</UIViewFormTabPane>
	<UIPopupWindow>
		<title>
			<UINavigationForm>Navigation</UINavigationForm>
		</title>
	</UIPopupWindow>
	<PopulateToMenu>
		<dialog>
			<label>
				<id>Id:</id>
				<name>Nom:</name>
				<lifecycle>Cycle de vie:</lifecycle>
				<isDeep>Récursif:</isDeep>
				<nodetypes>Types de noeud:</nodetypes>
				<description>Description:</description>
				<affectedNodeTypeNames>Types de noeud affectés:</affectedNodeTypeNames>
				<navigationNode>Noeud de navigation:</navigationNode>
				<index>Index:</index>
				<clickable>Cliquable:</clickable>
				<page>Page:</page>
				<childrenPage>Sous-Pages:</childrenPage>
			</label>
		</dialog>
	</PopulateToMenu>
	<UINavigationForm>
		<label>
			<Node>Dossier parent :</Node>
<Visible>Affiché :</Visible>
			<NavigationNodeInputSet>La navigation parent cible :</NavigationNodeInputSet>
<Index>Ordre d'Affichage :</Index>
<Clickable>Cliquable :</Clickable>
<NavigationControls>Paramètres de Navigation</NavigationControls>
<ListTargetPageFormInputSet>Page Liste :</ListTargetPageFormInputSet>
			<ListShowCLVByStringInput>Par :</ListShowCLVByStringInput>
<DetailTargetPageInputSet>Page Détail :</DetailTargetPageInputSet>
			<DetailShowCLVByStringInput>Par :</DetailShowCLVByStringInput>
			<List>Liste</List>
			<Detail>Détail</Detail>
		</label>
		<action>
<Save>#{word.save}</Save>
<Cancel>#{word.cancel}</Cancel>
		</action>
	</UINavigationForm>
	<Visible>
		<label>
			<VisibleEnable>Oui</VisibleEnable>
			<VisibleDisable>Non</VisibleDisable>
		</label>
	</Visible>
	<Clickable>
		<label>
			<ClickableEnable>Oui</ClickableEnable>
			<ClickableDisable>Non</ClickableDisable>
		</label>
	</Clickable>
	<NavigationNodeInputSet>
		<title>
<SelectNavigationNode>Sélectionnez un Noeud de Navigation</SelectNavigationNode>
			<RemoveNavigationNode>Réinitialiser</RemoveNavigationNode>
		</title>
	</NavigationNodeInputSet>
	<ListTargetPageFormInputSet>
		<title>
<SelectListTargetPage>Sélectionnez la Page Cible</SelectListTargetPage>
			<RemoveListTargetPage>Réinitialiser</RemoveListTargetPage>
		</title>
	</ListTargetPageFormInputSet>
	<DetailTargetPageInputSet>
		<title>
<SelectDetailTargetPage>Sélectionnez la Page Cible</SelectDetailTargetPage>
			<RemoveDetailTargetPage>Réinitialiser</RemoveDetailTargetPage>
		</title>
	</DetailTargetPageInputSet>
	<NavigationSelectorPopupWindow>
		<title>
<UINavigationSelector>Sélecteur de Navigation</UINavigationSelector>
		</title>
	</NavigationSelectorPopupWindow>
	<ListTargetPageSelectorPopupWindow>
		<title>
<UIPageSelector>Sélecteur de Page</UIPageSelector>
		</title>
	</ListTargetPageSelectorPopupWindow>
	<DetailTargetPageSelectorPopupWindow>
		<title>
<UIPageSelector>Sélecteur de Page</UIPageSelector>
		</title>
	</DetailTargetPageSelectorPopupWindow>
</bundle>