<node-navigation
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_2 http://www.gatein.org/xml/ns/gatein_objects_1_2"
    xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_2">
  <priority>1</priority>
  <page-nodes>
    <node>
      <uri>overview</uri>
      <name>overview</name>
      <label xml:lang="en">Home</label>
      <label xml:lang="fr">Présentation</label>
      <label xml:lang="it">Panoramica</label>
      <visibility>DISPLAYED</visibility>
      <page-reference>portal::acme::overview</page-reference>
    </node>
    
    <node>
      <uri>news</uri>
      <name>news</name>
      <label xml:lang="en">News</label>
      <label xml:lang="fr">Actualités</label>
      <label xml:lang="it">Notizie</label>
      <visibility>DISPLAYED</visibility>
      <page-reference>portal::acme::news</page-reference>
    </node>

    <node>
      <uri>products</uri>
      <name>products</name>
      <label xml:lang="en">Products</label>
      <label xml:lang="fr">Produits</label>
      <label xml:lang="it">Prodotti</label>
      <visibility>DISPLAYED</visibility>
      <page-reference>portal::acme::products</page-reference>
    </node>

    <node>
      <uri>contactus</uri>
      <name>contactus</name>
      <label xml:lang="en">Questions?</label>
      <label xml:lang="fr">Questions?</label>
      <label xml:lang="it">Domande?</label>
      <page-reference>portal::acme::contactus</page-reference>
      <node>
        <uri>contactus/confirmation</uri>
        <name>confirmation</name>
      <label xml:lang="en">Contact Form Confirmation</label>
      <label xml:lang="fr">Confirmation de contact</label>
      <label xml:lang="it">Conferma Form Contatti</label>
        <visibility>HIDDEN</visibility>
        <page-reference>portal::acme::confirmation</page-reference>
      </node>
    </node>

    <node>
      <uri>detail</uri>
      <name>detail</name>
      <label xml:lang="en">Details</label>
      <label xml:lang="fr">Détails</label>
      <label xml:lang="it">Dettagli</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::acme::detail</page-reference>
    </node>
	
	<node>
      <uri>catalog</uri>
      <name>catalog</name>
      <label xml:lang="en">Catalog</label>
      <label xml:lang="fr">Catalogue</label>
      <label xml:lang="it">Catalogo</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::acme::catalog</page-reference>
    </node>

    <node>
      <uri>printviewer</uri>
      <name>printviewer</name>
      <label xml:lang="en">Print page</label>
      <label xml:lang="fr">Page d'impression</label>
      <label xml:lang="it">Pagina Stampa</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::acme::printviewer</page-reference>
    </node>

    <node>
      <uri>searchResult</uri>
      <name>searchResult</name>
      <label xml:lang="en">Search</label>
      <label xml:lang="fr">Résultats de recherche</label>
      <label xml:lang="it">Cerca</label>
      <visibility>HIDDEN</visibility>
      <page-reference>portal::acme::searchResult</page-reference>
    </node>

    <node>
      <uri>newAccount</uri>
      <name>newAccount</name>
      <label xml:lang="en">New Account</label>
      <label xml:lang="fr">Nouveau compte</label>
      <label xml:lang="it">Nuovo Account</label>
      <visibility>HIDDEN</visibility>
      <page-reference>portal::acme::newAccount</page-reference>
    </node>

  </page-nodes>
</node-navigation>
