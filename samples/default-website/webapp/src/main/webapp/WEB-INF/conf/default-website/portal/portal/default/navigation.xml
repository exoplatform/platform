<node-navigation
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_2 http://www.gatein.org/xml/ns/gatein_objects_1_2"
    xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_2">
  <priority>1</priority>

  <page-nodes>
  <node>
      <uri>welcome</uri>
      <name>welcome</name>
      <label>#{portal.default.welcome}</label>
      <page-reference>portal::default::welcome</page-reference>
    </node>
	<node>
	    <uri>groupnavigation</uri>
	    <name>groupnavigation</name>
	    <label>#{portal.default.groupnavigation}</label>
      <visibility>SYSTEM</visibility>
	    <page-reference>portal::default::groupnavigation</page-reference>
	</node>
	<node>
      <uri>portalnavigation</uri>
      <name>portalnavigation</name>
      <label>#{portal.default.portalnavigation}</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::default::portalnavigation</page-reference>      
    </node>
  </page-nodes>
  </node-navigation>
